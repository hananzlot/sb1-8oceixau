{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bolt.beacontracker",
      "config": {
        "googleMapsApiKey": "AIzaSyD1kPT_peI9KIjBLoVb_iSIn-J7_-TNX3w"
      }
    },
    "android": {
      "package": "com.bolt.beacontracker",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD1kPT_peI9KIjBLoVb_iSIn-J7_-TNX3w"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-maps",
        {
          "iosGoogleMapsApiKey": "AIzaSyD1kPT_peI9KIjBLoVb_iSIn-J7_-TNX3w"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Beacon Tracker to use your location."
        }
      ],
      "expo-dev-client"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}